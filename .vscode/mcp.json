{
  "servers": {
    "eslint": {
      "type": "stdio",
      "command": "npx",
      "args": ["@eslint/mcp"]
    },
    "vitest": {
      "type": "stdio",
      "command": "npx",
      "args": ["@djankies/vitest-mcp"]
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest", "--executablePath=/usr/bin/chromium"]
    }
  },
  "inputs": []
}
