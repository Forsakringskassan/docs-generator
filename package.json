{
  "name": "@forsakringskassan/docs-generator",
  "version": "2.22.0",
  "description": "Documentation generator",
  "keywords": [
    "documentation"
  ],
  "homepage": "https://github.com/Forsakringskassan/docs-generator",
  "bugs": "https://github.com/Forsakringskassan/docs-generator/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/docs-generator.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "sideEffects": [
    "dist/runtime-bootstrap.js",
    "dist/runtime.js",
    "src/runtime/**/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./markdown": "./dist/markdown.js",
    "./runtime": "./dist/runtime.js",
    "./docs.css": "./dist/style/index.css",
    "./style.css": "./dist/style/index.css",
    "./style/*.css": "./dist/style/*.css"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "tsconfig-examples.json",
    "templates"
  ],
  "workspaces": [
    "internal/*",
    "docs"
  ],
  "scripts": {
    "prebuild": "npm run --workspaces --if-present build",
    "build": "tsc && node build.mjs",
    "postbuild": "node --enable-source-maps generate-docs.mjs && html-validate public",
    "build:docs": "node generate-docs.mjs",
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --fix .",
    "lint": "run-s prettier:check eslint stylelint",
    "prepack": "release-prepack --bundle --retain-scripts",
    "postpack": "release-postpack",
    "prepare": "husky",
    "prepublishOnly": "release-prepublish --bundle --retain-scripts",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postpublish": "release-postpublish",
    "start": "http-server public",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "pretest": "run-s prettier:check eslint stylelint",
    "test": "jest"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "lint-staged": {
    "*.{json,md,css,scss,html}": "prettier --write",
    "*.{js,cjs,mjs,vue,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "stylelint": {
    "extends": [
      "@forsakringskassan/stylelint-config"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.[jt]s",
      "internal/*/src/**/*.[jt]s",
      "packages/*/src/**/*.[jt]s",
      "!**/*.spec.[jt]s",
      "!**/*.d.ts",
      "!**/index.[jt]s",
      "!**/__fixtures__/**"
    ],
    "preset": "@forsakringskassan/jest-config",
    "projects": [
      {
        "displayName": "node",
        "preset": "@forsakringskassan/jest-config",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "<rootDir>/src/runtime"
        ]
      },
      {
        "displayName": "browser",
        "preset": "@forsakringskassan/jest-config",
        "testMatch": [
          "<rootDir>/src/runtime/**/*.spec.[jt]s"
        ],
        "testEnvironment": "jest-environment-jsdom"
      }
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "@leeoniya/ufuzzy": "1.0.18",
    "chokidar": "4.0.3",
    "cli-progress": "3.12.0",
    "dedent": "1.5.3",
    "diff": "7.0.0",
    "fastest-levenshtein": "1.0.16",
    "front-matter": "4.0.2",
    "fs-extra": "11.3.0",
    "glob": "11.0.1",
    "highlight.js": "11.11.1",
    "livereload-js": "4.0.2",
    "markdown-it": "14.1.0",
    "markdown-it-deflist": "3.0.0",
    "mermaid": "11.6.0",
    "nano-spawn": "0.2.0",
    "nunjucks": "3.2.4",
    "piscina": "4.9.2",
    "scroll-into-view-if-needed": "3.1.0",
    "semver": "7.7.1",
    "tiny-lr": "2.0.0",
    "vue-docgen-api": "4.79.2"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "1.4.2",
    "@forsakringskassan/docs-live-example": "1.5.6",
    "@forsakringskassan/eslint-config": "11.8.4",
    "@forsakringskassan/eslint-config-cli": "11.8.0",
    "@forsakringskassan/eslint-config-cypress": "11.8.0",
    "@forsakringskassan/eslint-config-jest": "11.8.4",
    "@forsakringskassan/eslint-config-typescript": "11.8.4",
    "@forsakringskassan/eslint-config-vue": "11.8.1",
    "@forsakringskassan/jest-config": "29.6.1",
    "@forsakringskassan/prettier-config": "3.0.5",
    "@forsakringskassan/stylelint-config": "2.4.3",
    "@html-validate/release-scripts": "6.8.0",
    "@microsoft/api-extractor": "7.52.2",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@types/babel__code-frame": "7.0.6",
    "@types/cli-progress": "3.11.6",
    "@types/d3": "7.4.3",
    "@types/dedent": "0.7.2",
    "@types/diff": "7.0.2",
    "@types/express": "5.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.14",
    "@types/markdown-it": "14.1.2",
    "@types/node": "20.17.29",
    "@types/nunjucks": "3.2.6",
    "@types/pug": "2.0.10",
    "@types/semver": "7.7.0",
    "cypress": "14.2.1",
    "cypress-html-validate": "7.1.0",
    "esbuild": "0.25.2",
    "express": "5.1.0",
    "html-validate": "9.5.2",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.5.0",
    "memfs": "4.17.0",
    "minimatch": "10.0.1",
    "npm-pkg-lint": "3.10.11",
    "npm-run-all2": "7.0.2",
    "rollup": "4.38.0",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-visualizer": "5.14.0",
    "sass": "1.86.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "esbuild": "0.x",
    "express": "^4 || ^5.0.0",
    "he": "^1.2",
    "sass": "^1.71",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 20.5"
  },
  "externalDependencies": [
    "@babel/code-frame",
    "livereload-js",
    "nunjucks",
    "piscina",
    "vue-docgen-api"
  ],
  "overrides": {
    "esbuild-plugin-vue3": {
      "esbuild": "$esbuild"
    },
    "nunjucks": {
      "chokidar": "$chokidar"
    }
  }
}
