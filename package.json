{
  "name": "@forsakringskassan/docs-generator",
  "version": "2.30.1",
  "description": "Documentation generator",
  "keywords": [
    "documentation"
  ],
  "homepage": "https://github.com/Forsakringskassan/docs-generator",
  "bugs": "https://github.com/Forsakringskassan/docs-generator/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/docs-generator.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "sideEffects": [
    "dist/runtime-bootstrap.mjs",
    "dist/runtime.mjs",
    "src/runtime/**/*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./markdown": {
      "types": "./dist/markdown.d.mts",
      "default": "./dist/markdown.mjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.mts",
      "default": "./dist/runtime.mjs"
    },
    "./docs": "./dist/style/index.css",
    "./docs.css": "./dist/style/index.css",
    "./style": "./dist/style/index.css",
    "./style.css": "./dist/style/index.css",
    "./style/*.css": "./dist/style/*.css"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "tsconfig-examples.json",
    "templates"
  ],
  "workspaces": [
    "internal/*",
    "docs"
  ],
  "scripts": {
    "prebuild": "npm run --workspaces --if-present build",
    "build": "tsc && node build.mjs",
    "postbuild": "node --enable-source-maps generate-docs.mjs && html-validate public",
    "build:docs": "node generate-docs.mjs",
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --fix .",
    "lint": "run-s prettier:check eslint stylelint",
    "prepack": "release-prepack --bundle --retain-scripts",
    "postpack": "release-postpack",
    "prepublishOnly": "release-prepublish --bundle --retain-scripts",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postpublish": "release-postpublish",
    "start": "http-server public",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "pretest": "run-s prettier:check eslint stylelint",
    "test": "jest"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "stylelint": {
    "extends": [
      "@forsakringskassan/stylelint-config"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.[jt]s",
      "internal/*/src/**/*.[jt]s",
      "packages/*/src/**/*.[jt]s",
      "!**/*.spec.[jt]s",
      "!**/*.d.ts",
      "!**/index.[jt]s",
      "!**/__fixtures__/**"
    ],
    "preset": "@forsakringskassan/jest-config",
    "projects": [
      {
        "displayName": "node",
        "preset": "@forsakringskassan/jest-config",
        "setupFiles": [
          "<rootDir>/jest.setup.ts"
        ],
        "testPathIgnorePatterns": [
          "/node_modules/",
          "<rootDir>/src/runtime"
        ]
      },
      {
        "displayName": "browser",
        "preset": "@forsakringskassan/jest-config",
        "testMatch": [
          "<rootDir>/src/runtime/**/*.spec.[jt]s"
        ],
        "testEnvironment": "jest-environment-jsdom"
      }
    ]
  },
  "overrides": {
    "esbuild-plugin-vue3": {
      "esbuild": "$esbuild"
    },
    "nunjucks": {
      "chokidar": "$chokidar"
    }
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/parser": "^7.0.0",
    "@babel/traverse": "^7.0.0",
    "@fontsource-variable/inter": "5.2.8",
    "@leeoniya/ufuzzy": "1.0.19",
    "chokidar": "4.0.3",
    "cli-progress": "3.12.0",
    "dedent": "1.7.0",
    "diff": "8.0.2",
    "fastest-levenshtein": "1.0.16",
    "front-matter": "4.0.2",
    "fs-extra": "11.3.2",
    "glob": "11.0.3",
    "highlight.js": "11.11.1",
    "i18next": "25.5.3",
    "livereload-js": "4.0.2",
    "markdown-it": "14.1.0",
    "markdown-it-deflist": "3.0.0",
    "mermaid": "11.12.0",
    "nunjucks": "3.2.4",
    "scroll-into-view-if-needed": "3.1.0",
    "semver": "7.7.3",
    "tiny-lr": "2.0.0",
    "vue-docgen-api": "4.79.2"
  },
  "devDependencies": {
    "@babel/types": "7.28.4",
    "@forsakringskassan/commitlint-config": "3.1.2",
    "@forsakringskassan/docs-live-example": "2.1.0",
    "@forsakringskassan/eslint-config": "13.0.8",
    "@forsakringskassan/eslint-config-cli": "13.0.4",
    "@forsakringskassan/eslint-config-cypress": "13.0.8",
    "@forsakringskassan/eslint-config-jest": "13.0.1",
    "@forsakringskassan/eslint-config-typescript": "13.0.8",
    "@forsakringskassan/eslint-config-typescript-typeinfo": "13.0.8",
    "@forsakringskassan/eslint-config-vue": "13.0.8",
    "@forsakringskassan/jest-config": "30.0.1",
    "@forsakringskassan/prettier-config": "3.1.7",
    "@forsakringskassan/sass-module-importer": "1.1.5",
    "@forsakringskassan/stylelint-config": "3.2.2",
    "@html-validate/release-scripts": "6.10.0",
    "@microsoft/api-extractor": "7.53.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.2",
    "@sidvind/rollup-plugin-worker": "1.0.2",
    "@types/babel__code-frame": "7.0.6",
    "@types/cli-progress": "3.11.6",
    "@types/d3": "7.4.3",
    "@types/dedent": "0.7.2",
    "@types/express": "5.0.3",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "30.0.0",
    "@types/markdown-it": "14.1.2",
    "@types/node": "20.19.19",
    "@types/nunjucks": "3.2.6",
    "@types/pug": "2.0.10",
    "@types/semver": "7.7.1",
    "cypress": "15.4.0",
    "cypress-html-validate": "8.1.0",
    "esbuild": "0.25.10",
    "express": "5.1.0",
    "html-validate": "10.1.1",
    "http-server": "14.1.1",
    "is-ci": "4.1.0",
    "jest-environment-jsdom": "30.2.0",
    "memfs": "4.49.0",
    "minimatch": "10.0.3",
    "npm-pkg-lint": "4.1.1",
    "npm-run-all2": "8.0.4",
    "rollup": "4.52.4",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-visualizer": "6.0.4",
    "sass": "1.93.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "esbuild": "0.x",
    "express": "^4 || ^5.0.0",
    "he": "^1.2",
    "sass": "^1.71",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 20.5"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "externalDependencies": [
    "@babel/code-frame",
    "@babel/parser",
    "@babel/traverse",
    "@fontsource-variable/inter",
    "livereload-js",
    "nunjucks",
    "piscina",
    "vue-docgen-api"
  ]
}
